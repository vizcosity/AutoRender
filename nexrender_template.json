{
  "template": {
    "composition": "Render 1080p [Change length]",
    "src": "DECLARE AE_TEMPLATE_PATH IN opts.env FILE"
  },
  "assets": [
    {
      "type": "script",
      "src": "DECLARE AE_SCRIPT_PATH in opts.env FILE"
    }
  ],
  "actions": {
        "postrender": [
            {
                "module": "nexrender-action-encode",
                "output": "DECLARE OUTPUT FOLDER IN opts.env FILE",
                "params": {
                    "-acodec": "aac",
                    "-ab": "128k",
                    "-ar": "44100",
                    "-vcodec": "libx264",
                    "-r": "29.97"
                }
            }
        ]
    }
}
